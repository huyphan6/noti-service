{
    "version": 2,
    "builds": [
        {
            "src": "/api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/check-expired-orders",
            "schedule": "0 9 * * *"
        }
    ]
}
